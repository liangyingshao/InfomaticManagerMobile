<view id="lab-apply-allot"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">分管领导分配</view></cu-custom><lab-steps bind:input="__e" vue-id="2" value="{{model}}" data-event-opts="{{[['^input',[['__set_model',['','model','$event',[]]]]]]}}" bind:__l="__l"></lab-steps><form><view class="cu-form-group margin-top"><view class="title">申请人</view><input disabled value="{{model.Owner}}"/></view><view class="cu-form-group"><view class="title">申请原因</view><input disabled value="{{model.ApplicationReason}}"/></view><view class="cu-form-group margin-top"><view class="title">起止时间</view><input disabled value="{{time}}"/></view><view class="cu-form-group"><view class="title">申请房间号</view><input disabled value="{{model.RoomName}}"/></view><view class="cu-form-group"><view class="title">指导老师</view><input disabled value="{{model.GuideTeacher}}"/></view><view class="cu-form-group"><view class="title">机房管理员</view><picker range="{{array}}" value="{{index}}" data-event-opts="{{[['change',[['managerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{array[index]}}</view></picker></view><view class="action-list cu-list grid col-3 margin-top margin-bottom"><view data-event-opts="{{[['tap',[['submit',['确认']]]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-roundcheckfill text-green"></view><text>确认分配</text></view><view data-event-opts="{{[['tap',[['submit',['修改']]]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-writefill text-red"></view><text>退回修改</text></view><view data-event-opts="{{[['tap',[['submit',['无法']]]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-roundclosefill text-red"></view><text>取消流程</text></view></view></form></view>