<view id="application-detail"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="/iuc/softwareInstall/v1/list" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">软件安装申请表</view></cu-custom><step-info bind:input="__e" vue-id="2" value="{{model}}" data-event-opts="{{[['^input',[['__set_model',['','model','$event',[]]]]]]}}" bind:__l="__l"></step-info><view class="action-list cu-list grid col-2 margin-top"><view data-event-opts="{{[['tap',[['changeState']]]]}}" class="cu-item" bindtap="__e"><view class="{{['text-blue',state==='detail'?'cuIcon-timefill':'cuIcon-form']}}"></view><text>{{state==='detail'?'流程跟踪':"查看申请表"}}</text></view><block wx:if="{{!currentStep.completed&&(isAdmin||isMyStep)}}"><view data-event-opts="{{[['tap',[['exeStep']]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-qrcode text-blue"></view><text>执行步骤</text></view></block></view><view hidden="{{!(state==='detail')}}" class="margin-tb"><view class="cu-form-group"><view class="title">申请人</view><input disabled value="{{model.Owner}}"/></view><view class="cu-form-group"><view class="title">联系方式</view><input disabled value="{{model.Telephone}}"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国大陆</view></view></view><view class="cu-form-group"><view class="title">所属部门</view><input disabled value="{{model.BelongDepart}}"/></view><view class="cu-form-group"><view class="title">申请时间</view><input disabled="true" value="{{model.CreatedTime}}"/></view><view class="cu-form-group margin-top"><view class="title">选择实验室</view><input disabled value="{{model.RoomName}}"/></view><view class="cu-form-group"><view class="title">待装软件</view><input disabled value="{{model.ApplicationName}}"/></view><view class="cu-form-group"><view class="title">软件版本</view><input disabled value="{{model.Version||'未设置'}}"/></view><view class="cu-form-group"><view class="title">下载地址</view><input disabled value="{{model.Download||'未设置'}}"/></view><block wx:if="{{model.ReviewOpinion}}"><view class="margin-top"><view class="cu-form-group"><view class="title">安装人</view><input disabled value="{{model.HandlerName||'未设置'}}"/></view><view class="cu-form-group"><view class="title">维修部意见</view><input disabled value="{{model.ReviewOpinion||'未设置'}}"/></view></view></block><block wx:if="{{model.HandleOpinion}}"><view class="margin-top"><view class="cu-form-group"><view class="title">安装意见</view><input disabled value="{{model.HandleOpinion||'未设置'}}"/></view><view class="cu-form-group"><view class="title">安装时间</view><input disabled value="{{model.HandleTime||'未设置'}}"/></view></view></block><block wx:if="{{model.CheckOpinion}}"><view class="margin-top"><view class="cu-form-group"><view class="title">确认人</view><input disabled value="{{model.Checker||'未设置'}}"/></view><view class="cu-form-group"><view class="title">确认意见</view><input disabled value="{{model.CheckOpinion||'未设置'}}"/></view><view class="cu-form-group"><view class="title">确认时间</view><input disabled value="{{model.CheckTime||'未设置'}}"/></view></view></block></view><view hidden="{{!(state==='timeline')}}" class="margin-top"><block wx:for="{{$root.l1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-timeline"><view class="cu-time">{{item.$orig.Key}}</view><block wx:for="{{item.l0}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="{{['cu-item','text-'+stepColor[v.$orig.State]]}}"><view class="content"><view class="cu-capsule radius"><view class="{{['cu-tag','bg-'+stepColor[v.$orig.State]]}}">{{v.$orig.ExecutorName+stepInfo[v.$orig.State]}}</view><view class="{{['cu-tag','line-'+stepColor[v.$orig.State]]}}">{{v.$orig.Time}}</view></view><view class="margin-top">{{''+v.$orig.StepName+''}}</view><view class="text-grey text-sm margin-top"><block wx:if="{{v.m0}}">{{''+(v.$orig.ExecutorName?v.$orig.ExecutorName+' 正在进行中':"正在等待接手")+''}}</block><block wx:else><block wx:if="{{[2,3]}}">{{'由'+v.$orig.Operator+"于"+v.$orig.CreatedOn+'完成'}}</block><block wx:else>{{'由'+v.$orig.Operator+"于"+v.$orig.CreatedOn+'取消'}}</block></block></view></view></view></block></view></block></view></view>