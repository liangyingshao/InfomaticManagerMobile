<view id="software-confirm"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="/iuc/softwareInstall/v1/list" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">软件安装申请表</view></cu-custom><step-info bind:input="__e" vue-id="2" value="{{model}}" data-event-opts="{{[['^input',[['__set_model',['','model','$event',[]]]]]]}}" bind:__l="__l"></step-info><form><view class="cu-form-group margin-top"><view class="title">申请人</view><input disabled value="{{model.Owner}}"/></view><view class="cu-form-group"><view class="title">联系方式</view><input disabled value="{{model.Telephone}}"/><view class="cu-capsule radius"><view class="cu-tag bg-blue ">+86</view><view class="cu-tag line-blue">中国大陆</view></view></view><view class="cu-form-group"><view class="title">所属部门</view><input disabled value="{{model.BelongDepart}}"/></view><view class="cu-form-group"><view class="title">申请时间</view><input disabled="true" value="{{model.CreatedTime}}"/></view><view class="cu-form-group margin-top"><view class="title">选择实验室</view><input disabled value="{{model.RoomName}}"/></view><view class="cu-form-group"><view class="title">待装软件</view><input disabled value="{{model.ApplicationName}}"/></view><view class="cu-form-group"><view class="title">软件版本</view><input disabled value="{{model.Version||'未设置'}}"/></view><view class="cu-form-group"><view class="title">下载地址</view><input disabled value="{{model.Download||'未设置'}}"/></view><view class="cu-form-group"><view class="title">安装人</view><input disabled value="{{model.HandlerName||'未设置'}}"/></view><view class="cu-form-group"><view class="title">安装时间</view><input disabled value="{{model.HandleTime||'未设置'}}"/></view><view class="action-list cu-list grid col-2 margin-top margin-bottom"><view data-event-opts="{{[['tap',[['submit',['']]]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-roundcheckfill text-green"></view><text>安装完成</text></view><view data-event-opts="{{[['tap',[['submit',['安装未完成','您是否已经与安装人员线下沟通，确认软件无法安装？']]]]]}}" class="cu-item" bindtap="__e"><view class="cuIcon-roundclosefill text-red"></view><text>未完成</text></view></view></form></view>